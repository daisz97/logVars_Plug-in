{
	"name": "loggingvars",
	"displayName": "LoggingVars",
	"description": "To recommend logging variables while developing Java projects",
	"version": "0.0.2",
	"publisher": "Daishaozhi",
	"engines": {
		"vscode": "^1.61.0"
	},
	"repository": {},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:loggingvars.logvars"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "loggingvars.logvars",
				"title": "log vars"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "loggingvars.logvars",
					"group": "6_recommdendVars"
				}
			]
		},
		"keybindings":[
			{
				"command": "loggingvars.logvars",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
